{
  "expo": {
    "name": "PORELO",
    "slug": "porelo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/p_tronc.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo/porelo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.porelo.mobile",
      "buildNumber": "1"
    },
