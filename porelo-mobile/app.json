      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/p_tronc.png",
        "backgroundColor": "#B6D8F2"
      },
      "package": "com.porelo.mobile",
      "versionCode": 1,
      "icon": "./assets/logo/p_tronc.png",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "favicon": "./assets/logo/p_tronc.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "89eb2f3b-fc30-4113-b726-f445353f791e"
      }
    }
  }
